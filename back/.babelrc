{
  "presets": ["@babel/env", "@babel/preset-typescript"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "library": "./src/library",
          "api": "./src/api",
          "config": "./src/config",
          "helper": "./src/helper"
        }
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}
