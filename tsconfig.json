{
  "compilerOptions": {
    "baseUrl": "src",
		"paths": {
      "public/*": [
        "public/*"
      ],
      "app/*": [
        "src/app/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "containers/*": [
        "src/containers/*"
      ],
      "helpers/*": [
        "src/helpers/*"
      ],
		},
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src/**/*"
  ],
  "exclude":["node_modules"]
}
